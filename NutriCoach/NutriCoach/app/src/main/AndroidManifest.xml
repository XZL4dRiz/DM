<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoDM"
        tools:targetApi="31">

        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".PrincipalActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.ProyectoDM">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".FormularioActivity"
            android:exported="false"
            android:theme="@style/Theme.ProyectoDM">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PerfilActivity" />
        </activity>
        <activity
            android:name=".PerfilActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.ProyectoDM">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".VerActivity"
            android:exported="false"
            android:parentActivityName=".PrincipalActivity"
            android:theme="@style/Theme.ProyectoDM">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PrincipalActivity" />
        </activity>
        <activity
            android:name=".VerEjerciciosActivity"
            android:exported="false"
            android:parentActivityName=".PrincipalEjerciciosActivity"
            android:theme="@style/Theme.ProyectoDM">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PrincipalActivity" />
        </activity>
        <activity
            android:name=".PrincipalEjerciciosActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.ProyectoDM">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".NuevoEjercicioActivity"
            android:exported="false"
            android:parentActivityName=".PrincipalEjerciciosActivity"
            android:theme="@style/Theme.ProyectoDM">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".PrincipalActivity" />
        </activity>
        <activity
            android:name=".InicioActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>